    "@mui/material": "^6.4.1",
    "@mui/styles": "^6.4.1",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.1.2",
    "react-scripts": "^5.0.1",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "axios": "^1.8.3",
    "dotenv": "^16.4.7",
    "firebase": "^11.2.0",
